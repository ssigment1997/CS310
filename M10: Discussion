#include <iostream>
using namespace std;

int main() {
    int numCats = 3;
    int *catAges;

    catAges = new int[numCats];

    // Set ages
    catAges[0] = 2;
    catAges[1] = 4;
    catAges[2] = 5;

    cout << "The age of cat 1 is: " << *catAges[0] << endl; //  Error 1

    delete catAges; //  Error 2

    return 0;
}
